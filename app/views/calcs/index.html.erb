<p id="notice"><%= notice %></p>

<h1>Calcs</h1>

<table>
  <thead>
    <tr>
      <th>Dividend</th>
      <th>Divider</th>
      <th>Quotient</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @calcs.each do |calc| %>
      <tr>
        <td><%= calc.dividend %></td>
        <td><%= calc.divider %></td>
        <td><%= calc.quotient %></td>
        <td><%= link_to 'Show', calc %></td>
        <td><%= link_to 'Edit', edit_calc_path(calc) %></td>
        <td><%= link_to 'Destroy', calc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Calc', new_calc_path %>
<%= link_to 'Back', root_path %>
